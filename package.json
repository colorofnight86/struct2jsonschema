{
	"name": "struct2jsonschema",
	"displayName": "struct2jsonschema",
	"description": "以jsonchema的方式可视化显示c语言结构体",
	"keywords": [
		"c struct",
		"JsonShema"
	],
	"version": "1.0.2",
	"publisher": "colorofnight",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"icon":"img/icon.png",
	"activationEvents": [
		"onCommand:helloworld.helloWorld",
		"onCommand:webview.jsonview"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command":"webview.jsonview",
				"title": "view code in json",
				"icon":{
					"light": "./img/json.svg",
					"dark": "./img/json.svg"
				}
			}
		],
		"keybindings": [
        ],
		"menus": {
            "editor/context": [
				{
                    "when": "resourceLangId == c",
                    "command": "webview.jsonview",
                    "group": "navigation"
                }
            ],
			"editor/title": [
				{
					"when": "resourceLangId == c",
					"command": "webview.jsonview",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/colorofnight86/struct2jsonschema"
    }
}
